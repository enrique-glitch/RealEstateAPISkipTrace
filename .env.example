# RealEstate API configuration
REAPI_BASE_URL=https://api.realestateapi.com
REAPI_SEARCH_PATH=/v1/property/search
REAPI_DETAIL_BULK_PATH=/v1/property/detail/bulk
REAPI_PROPERTY_API_KEY=YOUR_PROPERTY_API_KEY_HERE

# App configuration
FLASK_ENV=development
SECRET_KEY=change-this-secret
CACHE_TTL_SECONDS=604800
# If true, do not call detail/bulk automatically unless user requests; prefer ids_only
COST_SAVER_MODE=true
# If true, send live=false on search calls to avoid consuming credits (if supported by your key)
TEST_MODE=false
